<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GenerateTripActivity">

    <EditText
        android:id="@+id/enterAddTv1"
        android:layout_width="229dp"
        android:layout_height="36dp"
        android:layout_marginTop="150dp"
        android:background="#AEFFFFFF"
        android:hint="Enter 1st place"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/enterAddTv2"
        android:layout_width="229dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="#AEFFFFFF"
        android:hint="Enter 2nd place"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enterAddTv1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="ADD MORE PLACES"
        app:layout_constraintEnd_toEndOf="@+id/enterAddTv8"
        app:layout_constraintTop_toBottomOf="@+id/enterAddTv8" />


</androidx.constraintlayout.widget.ConstraintLayout>